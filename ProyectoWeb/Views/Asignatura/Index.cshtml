@model Asignatura //Definimos el modelo a usar en la plantilla, necesario para que sea Fuertemente tipado

@{
    ViewData["Title"] = "Informacion Asignatura";
    Layout = "Simple"; // Se hace por defecto si no se pone nada
}

<h1>@ViewData["Title"]</h1>
<strong>Desde la vista normal</strong>
<h2>Nombre: @Model.Nombre</h2>
<p>ID: @Model.Id</p>

<p><i>@ViewBag.CosaDinamica</i></p>
<p><i>@ViewBag.fecha</i></p>

<strong>Desde la vista parcial</strong>
<partial name="ObjetoEscuelaSimple"/>

@*
ViewBag y ViewData son la misma forma de acceder a los datos dinamicos de la clase
*@


